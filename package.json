{
  "name": "freelancing-platform-backend",
  "version": "1.0.0",
  "description": "Backend for freelancing platform with Client and Freelancer roles",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seed.js",
